  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug Generator",
      "request": "launch",
      "type": "dart",
      "args": [
        "i18n",
        "-i",
        "source/i18n/",
        "-e",
        "lib/i18n.g.dart",
        "--serialize",
      ]
    },
    {
      "name": "build_runner",
      "request": "launch",
      "program": ".dart_tool/build/entrypoint/build.dart",
      "type": "dart",
      "args": [
        "build"
      ]
    }
  ]
}